SELECT DISTINCT X_ID, X_NAME, STATE, Y_NAME, Y_TYPE_NAME, 
                X_CONTACT_ROLE, FYE, OPINION_DATE, LAST_AUDIT_INFO, 
                CONTACT_UPD_ID, TRUNC(CONTACT_UPD_DATE) AS CNTCT_UPD_DATE, 
				NEXT_ACTIVITY_DATE, NEXT_ACTIVITY_DESC
FROM Z
WHERE 1=1
    AND Y_TYPE_NAME = 'Audit'
    AND NEXT_ACTIVITY_DATE BETWEEN '31-MAR-2017' AND '30-APR-2017'
    AND EXTRACT(YEAR FROM CONTACT_UPD_DATE) < 2014
    AND LAST_AUDIT_INFO IS NOT NULL
;
